            "lines": [
                {
                    "chords": "           G             C/G",
                    "lyrics": "O Lord my God, When I in awesome wonder,"
                },
                {
                    "chords": "         G                  D/F#      G",
                    "lyrics": "consider all the Worlds Thy hand hath made."
                },
                {
                    "chords": "           G                C/G",
                    "lyrics": "I see the stars, I hear the rolling thunder"
                },
                {
                    "chords": "               G            D/F#     G",
                    "lyrics": "Thy pow'r throughout the universe displayed"
                }
            ]
        },
